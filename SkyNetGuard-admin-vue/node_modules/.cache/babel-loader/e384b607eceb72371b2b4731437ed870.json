tThree();\r\n  },\r\n  methods: {\r\n    initThree() {\r\n      const container = this.$refs.cubeContainer;\r\n\r\n      // 设置场景\r\n      const scene = new THREE.Scene();\r\n\r\n      // 设置相机\r\n      const camera = new THREE.PerspectiveCamera(75, container.clientWidth / container.clientHeight, 0.1, 1000);\r\n      camera.position.z = 5;\r\n\r\n      // 设置渲染器\r\n      const renderer = new THREE.WebGLRenderer();\r\n      renderer.setSize(container.clientWidth, container.clientHeight);\r\n      container.appendChild(renderer.domElement);\r\n\r\n      // 创建立方体\r\n      const geometry = new THREE.BoxGeometry();\r\n      const material = new THREE.MeshBasicMaterial({ color: 0x00ff00, wireframe: true });\r\n      const cube = new THREE.Mesh(geometry, material);\r\n      scene.add(cube);\r\n\r\n      // 设置控制器，使相机可以通过鼠标拖动旋转\r\n      const controls = new OrbitControls(camera, renderer.domElement);\r\n      controls.enableDamping = true; // 添加阻尼效果，增强体验感\r\n      controls.dampingFactor = 0.05;\r\n      controls.enableZoom = true; // 允许缩放\r\n\r\n      // 渲染场景\r\n      const animate = function () {\r\n        requestAnimationFrame(animate);\r\n\r\n        controls.update(); // 更新控制器\r\n\r\n        renderer.render(scene, camera);\r\n      };\r\n\r\n      animate();\r\n\r\n      // 监听窗口大小变化，更新渲染器和相机的比例\r\n      window.addEventListener('resize', () => {\r\n        renderer.setSize(container.clientWidth, container.clientHeight);\r\n        camera.aspect = container.clientWidth / container.clientHeight;\r\n        camera.updateProjectionMatrix();\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.cube-container {\r\n  width: 100%;\r\n  height: 100vh;\r\n  overflow: hidden;\r\n}\r\n</style>\r\n"],"mappings":";;;;;AAKA,YAAAA,KAAA;AACA,SAAAC,aAAA;AAEA;EACAC,IAAA;EACAC,OAAA,WAAAA,QAAA;IACA,KAAAC,SAAA;EACA;EACAC,OAAA;IACAD,SAAA,WAAAA,UAAA;MACA,IAAAE,SAAA,QAAAC,KAAA,CAAAC,aAAA;;MAEA;MACA,IAAAC,KAAA,OAAAT,KAAA,CAAAU,KAAA;;MAEA;MACA,IAAAC,MAAA,OAAAX,KAAA,CAAAY,iBAAA,KA