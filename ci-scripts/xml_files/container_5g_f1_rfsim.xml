<testCaseList>
        <htmlTabRef>rfsim-5gnr-f1</htmlTabRef>
        <htmlTabName>CU-DU F1 split</htmlTabName>
        <htmlTabIcon>wrench</htmlTabIcon>
        <TestCaseRequestedList>
 111111
 100021
 000021
 000022
 000023
 020021
 030020
 030021
